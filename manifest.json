{
    "manifest_version": 3,
    "name": "Canvas Extension",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "https://www.jacobspaulsen.com/wp-content/uploads/2012/06/split-testing.jpg"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "storage",
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["https://canvas.uoregon.edu"]  
      }
    ],
    "host_permissions": [
      "https://*.instructure.com/*"
    ]
  }
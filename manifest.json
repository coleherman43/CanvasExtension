{
    "manifest_version": 3,
    "name": "Canvas Extension",
    "version": "1.0",
    "description": "A Chrome Extension to fetch Canvas data.",
    "action": {
      "default_popup": "popup/popup.html"
    },
    "background": {
      "service_worker": "background.js"   
    },
    "permissions": [
      "storage",
      "activeTab"
    ],
    "host_permissions": [
      "https://*.instructure.com/*" 
    ],
    "icons": {
    "16": "./images/logo.png",
    "48": "./images/logo.png",
    "128": "./images/logo.png"
    }
  }
  